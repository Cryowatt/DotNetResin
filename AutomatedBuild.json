{
    "name": "${ResinMachineName}-dotnet",
    "namespace": "cryowatt",
    "description": "Resin.io base image for dotnet core.",
    "vcs_repo_name": "cryowatt/dotnetresin",
    "provider": "github",
    "dockerhub_repo_name": "cryowatt/${ResinMachineName}-dotnet",
    "is_private": false,
    "active": true,
    "build_tags": [{
        "name": "{sourceref}-deps",
        "source_type": "Tag",
        "source_name": "/.*/",
        "dockerfile_location": "/devices/${ResinMachineName}/deps.Dockerfile"
    },
    {
        "name": "{sourceref}",
        "source_type": "Tag",
        "source_name": "/.*/",
        "dockerfile_location": "/devices/${ResinMachineName}/runtime.Dockerfile"
    }]
}